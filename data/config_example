# Example SSH config
#
# $ man ssh_config

Host hostname1
#tag: tagValue1,tagValue2,tagN
    User user
    HostName hello.world

Host terminalcoffee
#tag: shops
    User primeagen
    HostName terminal.shop

host srv1
#tag: prod
    User linus
    HostName prod.srv.linux.com

host srv2
#tag: prod
    User skyper
    HostName hackme.phrack.org
    LocalForward 5900 0:5900

host srv3
#tag: test
    User test
    HostName test.phrack.org

host srv4
#tag: sandbox
    User root
    HostName sandbox.segfault.net
    LocalForward 5900 0:5900
